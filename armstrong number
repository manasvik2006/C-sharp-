using System;

class armstrong
{
    static void Main()
    {
        Console.WriteLine("Enter a number");
        int a = Convert.ToInt32(Console.ReadLine());
        int sum = 0, temp = a;
        while(temp>0)
        {
             int digit = temp % 10;
             sum += digit* digit * digit;
             temp /= 10;
        }
        if (sum == a)
        {
            Console.WriteLine("Armstrong number");
        }
        else
        {
             Console.WriteLine("Not an armstrong");
        }
    }
}
